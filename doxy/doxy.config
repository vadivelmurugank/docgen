GENERATE_TREEVIEW      = YES
USE_INLINE_TREES       = YES
GENERATE_LATEX         = NO
MACRO_EXPANSION        = YES

########
## VERBOSITY OPTIONS
#########

GENERATE_HTML          = YES
HTML_OUTPUT            = html

########
### DOT Tool configuration
#########

HAVE_DOT               = YES
DOT_FONTSIZE           = 12
DOT_IMAGE_FORMAT       = jpg
DOT_GRAPH_MAX_NODES    = 200
DOT_CLEANUP            = NO
DOT_PATH               = 

CLASS_DIAGRAMS         = YES
CLASS_GRAPH            = YES
COLLABORATION_GRAPH    = YES
UML_LOOK               = YES
TEMPLATE_RELATIONS     = YES
INCLUDE_GRAPH          = NO
INCLUDED_BY_GRAPH      = NO
DIRECTORY_GRAPH        = NO
CALL_GRAPH             = YES
CALLER_GRAPH           = YES
GRAPHICAL_HIERARCHY    = YES
########
# HEADER/FOOTER OPTIONS
#########

HTML_HEADER            = ${HOME}/docgen/doxy/template/myheader.html
HTML_FOOTER            = ${HOME}/docgen/doxy/template/myfooter.html
HTML_STYLESHEET        = ${HOME}/docgen/doxy/template/mystylesheet.css

########
# PROJECT OPTIONS
#########

PROJECT_NAME           = project
INPUT                  = .
