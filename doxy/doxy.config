
###########################################
# Local Doxyfile Configs
##########################################

# Generate default config
    # doxygen -g config.file

# Generate default footer/header
    # doxygen -w html header.html footer.html stylesheet.css ../file.doxy 
    # doxygen -w latex header.tex footer.tex doxygen.sty <<config.file>>
    # doxygen -w rtf rtfstyle.cfg

# Generate Project Specifics
    # python3 -m peekextn --doxy

#######
## GLOBAL options
########
EXTRACT_ALL            = YES
FULL_PATH_NAMES        = NO
TAB_SIZE               = 4
CREATE_SUBDIRS         = YES

RECURSIVE              = YES
SOURCE_BROWSER         = NO
INLINE_SOURCES         = YES
REFERENCED_BY_RELATION = YES
